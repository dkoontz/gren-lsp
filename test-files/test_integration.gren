module TestIntegration exposing (..)

type alias Config = 
    { apiUrl : String
    , timeout : Int
    }

type RequestStatus = Pending | Success String | Error String

fetchData : Config -> String -> Task Error String
fetchData config url = Http.get { url = config.apiUrl ++ url }

processResponse : RequestStatus -> String
processResponse status =
    case status of
        Pending -> "Loading..."
        Success data -> "Got: " ++ data  
        Error msg -> "Failed: " ++ msg